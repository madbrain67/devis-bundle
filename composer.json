{
    "name": "madbrain67/devis-bundle",
    "type": "symfony-bundle",
    "description": "Symfony DevisBundle",
    "keywords": ["generator", "code generator", "devis"],
    "license": "MIT",
    "authors": [
        {
            "name": "FERRERO Franck",
            "email": "ferrerofranck@plateformweb.com",
            "homepage": "https://plateformweb.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "dompdf/dompdf": "^1.1",
        "symfony/config": "^4.4 || ^5.0",
        "symfony/dependency-injection": "^3.4 || ^4.2 || ^5.0",
        "symfony/expression-language": "^3.4 || ^4.2 || ^5.0",
        "symfony/http-foundation": "^3.4 || ^4.2 || ^5.0",
        "symfony/http-kernel": "^5.4",
        "symfony/security-core": "^5.4",
        "symfony/templating": "^3.4 || ^4.2 || ^5.0",
        "symfony/translation": "^3.4 || ^4.2 || ^5.0",
        "symfony/twig-bridge": "^3.4 || ^4.2 || ^5.0",
        "symfony/twig-bundle": "^3.4 || ^4.2 || ^5.0",
        "symfony/yaml": "^3.4 || ^4.2 || ^5.0",
        "twig/twig": "^2.6 || ^3.0"
    },
    "require-dev": {
        "cr/hashcli": "dev-master",
        "phpunit/phpunit": "^7.5",
        "symfony/console": "^5.4",
        "symfony/debug-bundle": "^5.4",
        "symfony/framework-bundle": "^5.4",
        "symfony/maker-bundle": "^1.36",
        "symfony/process": "^5.4",
        "symfony/runtime": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Madbrain67\\DevisBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Madbrain67\\DevisBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
